{
  "name": "style-hook",
  "displayName": "style-hook",
  "description": "Syntax Highlighting and IntelliSense for Style-Hook",
  "version": "0.1.1",
  "publisher": "coppy",
  "icon": "logo.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/style-hook/vscode-style-hook"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,md,json,css}'",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test ./test"
  },
  "contributes": {
    "languages": [
      {
        "id": "source.css.style",
        "aliases": [
          "CSS (Style Hook)"
        ],
        "configuration": "./css.style.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "source.css.style",
        "scopeName": "source.css.style",
        "path": "./syntaxes/css.style.json"
      },
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.js.jsx",
          "source.tsx",
          "source.vue"
        ],
        "scopeName": "style-hook",
        "path": "./syntaxes/style-hook.json",
        "embeddedLanguages": {
          "source.css.scss": "css",
          "meta.embedded.line.ts": "typescript"
        }
      }
    ],
    "typescriptServerPlugins": [
      {
        "name": "typescript-style-hook-plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "dependencies": {
    "typescript-style-hook-plugin": "*"
  },
  "devDependencies": {
    "prettier": "^1.14.3",
    "prettier-cli": "^0.1.0",
    "vscode": "^1.1.26"
  }
}
